pipeline {
    agent any
    stages {
        stage('test') {
            steps {
                    mvn -B -D${USER} -D${PASS} -f ./test clean release:clean release:prepare release:perform -DlocalCheckout=true -DdryRun=false -Dscm.user=${USER} -dscm.pass=${PASS}
                }
            }
        }
    }
}