pipeline {
    agent any
    
    stages {
        stage('test') {
            steps {
                    mvn -B  -f ./test clean release:clean release:prepare release:perform -DlocalCheckout=true -DdryRun=false -Dscm.user='RITESH_TOKEN' -dscm.pass='RITESH_TOKEN'
                }
            }
        }
    }
